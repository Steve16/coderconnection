<%= form_for(@proposals) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
  	
    <%= f.text_area :description, placeholder: "Describe the project or job..." %>
    <%= f.text_area :title, placeholder: "Proposal sdvsTitle..." %>
  </div>
  <%= f.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>